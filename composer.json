{
  "name": "bavix/cms",
  "description": "Bavix CMS.",
  "keywords": [
    "cms",
    "site editor",
    "editor",
    "site"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@gitlab.com:bavix/laravel-nova.git"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "ext-pdo": "*",
    "coderello/laravel-nova-lang": "^1.3",
    "davejamesmiller/laravel-breadcrumbs": "^5.3",
    "gregoriohc/laravel-nova-theme-responsive": "^0.8",
    "laravel/nova": "^2.7",
    "doctrine/dbal": "^2.10",
    "ezyang/htmlpurifier": "^4.12",
    "fideloper/proxy": "^4.2",
    "intervention/image": "^2.5",
    "laravel/framework": "^6.6",
    "laravel/tinker": "^2.0",
    "laravelium/sitemap": "^6.0",
    "mgp25/instagram-php": "^7.0",
    "predis/predis": "^1.1",
    "ps/image-optimizer": "^2.0",
    "ralouphie/mimey": "^2.1",
    "spatie/image-optimizer": "^1.2",
    "spatie/laravel-tags": "^2.5"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.6",
    "beyondcode/laravel-dump-server": "^1.3",
    "facade/ignition": "^1.12",
    "fzaninotto/faker": "~1.9",
    "mockery/mockery": "^1.3",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.4"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "files": [
      "helpers/sape.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan nova-lang:publish ru --force"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "@php artisan nova:publish"
    ]
  }
}
