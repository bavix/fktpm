{
  "name": "bavix/cms",
  "description": "Bavix CMS.",
  "keywords": [
    "cms",
    "site editor",
    "editor",
    "site"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1",
    "barryvdh/laravel-debugbar": "^3.1",
    "bavix/advanced-html-dom": "^1.0",
    "bavix/gearman": "^1.0",
    "bavix/laravel-helpers": "^1.0",
    "bavix/laravel-worker-command": "^1.0",
    "davejamesmiller/laravel-breadcrumbs": "^3.0",
    "doctrine/dbal": "^2.5",
    "ezyang/htmlpurifier": "^4.9",
    "intervention/image": "^2.3",
    "laravel-admin-ext/config": "^1.0",
    "laravel-admin-ext/helpers": "^1.0",
    "laravel-admin-ext/log-viewer": "^1.0",
    "laravel-admin-ext/media-manager": "^1.0",
    "laravel-admin-ext/scheduling": "^1.0",
    "bavix/laravel-middleware-ifindex": "^1.0",
    "laravel/framework": "5.6.*",
    "laravelrus/localized-carbon": "dev-master",
    "mgp25/instagram-php": "^5.0",
    "predis/predis": "^1.1",
    "ralouphie/mimey": "^1.0",
    "roumen/sitemap": "^2.7",
    "spatie/image-optimizer": "^1.0",
    "spatie/laravel-paginateroute": "^2.5",
    "spatie/laravel-partialcache": "^1.2",
    "yab/laravel-scout-mysql-driver": "^2.0",
    "ps/image-optimizer": "^1.1"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.4",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "files": [
      "helpers/quick.php",
      "helpers/sape.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
