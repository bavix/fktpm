{
  "name": "bavix/cms",
  "description": "Bavix CMS.",
  "keywords": [
    "cms",
    "site editor",
    "editor",
    "site"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.0",
    "bavix/diff": "^1.0",
    "bavix/sdk": "^1.0",
    "davejamesmiller/laravel-breadcrumbs": "^3.0",
    "doctrine/dbal": "^2.5",
    "encore/laravel-admin": "^1.5",
    "ezyang/htmlpurifier": "^4.9",
    "fideloper/proxy": "~3.3",
    "intervention/image": "^2.3",
    "laravel-admin-ext/config": "^1.0",
    "laravel-admin-ext/helpers": "^1.0",
    "laravel-admin-ext/media-manager": "^1.0",
    "laravel/framework": "5.5.*",
    "laravel/tinker": "~1.0",
    "laravelrus/localized-carbon": "dev-master",
    "rtconner/laravel-tagging": "^2.2",
    "simplesoftwareio/simple-qrcode": "~1",
    "spatie/laravel-paginateroute": "^2.5",
    "spatie/laravel-sitemap": "^3.2",
    "yab/laravel-scout-mysql-driver": "^2.0"
  },
  "require-dev": {
    "tamayo/laravel-scout-elastic": "^3.0",
    "ps/image-optimizer": "^1.1",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~6.0",
    "bavix/gearman": "^1.0",
    "filp/whoops": "~2.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "files": [
      "helpers/quick.php"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
